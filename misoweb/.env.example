# MQTT Configuration
# Copy this file to .env.local and fill in your actual values

# HiveMQ Cloud MQTT Broker URL (without protocol)
NEXT_PUBLIC_MQTT_URL=your-hivemq-url.s1.eu.hivemq.cloud

# MQTT Port (8883 for WSS, 8884 for websockets)
NEXT_PUBLIC_MQTT_PORT=8883

# MQTT Authentication
NEXT_PUBLIC_MQTT_USERNAME=your-username
NEXT_PUBLIC_MQTT_PASSWORD=your-password

# MQTT Topic to subscribe to
NEXT_PUBLIC_MQTT_TOPIC=misogate/pub
